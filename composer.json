{
    "name": "runroom/redirection-bundle",
    "type": "symfony-bundle",
    "description": "Manage redirections on the Sonata Admin interface",
    "keywords": ["redirection", "symfony", "doctrine", "admin", "sonata"],
    "homepage": "https://github.com/runroom/RedirectionBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Runroom",
            "email": "runroom@runroom.com",
            "homepage": "https://www.runroom.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "doctrine/orm": "^2.6",
        "sonata-project/admin-bundle": "^3.49",
        "symfony/dependency-injection": "^3.4 || ^4.3",
        "symfony/doctrine-bridge": "^3.4 || ^4.3",
        "symfony/event-dispatcher": "^3.4 || ^4.3",
        "symfony/form": "^3.4 || ^4.3",
        "symfony/http-foundation": "^3.4 || ^4.3",
        "symfony/http-kernel": "^3.4 || ^4.3",
        "symfony/validator": "^3.4 || ^4.3",
        "symfony/yaml": "^3.4 || ^4.3"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^4.3"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Runroom\\RedirectionBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Runroom\\RedirectionBundle\\Tests\\": "tests/"
        }
    }
}
